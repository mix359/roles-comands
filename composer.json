{
    "name": "mix359/roles-commands",
    "description": "An artisan interface to the roles laravel package",
    "keywords": [
        "league",
        "roles",
        "artisan",
        "console",
        "command"
    ],
    "homepage": "https://github.com/mix359/roles-commands",
    "license": "MIT",
    "authors": [
        {
            "name": "Mix",
            "email": "mix@mixglobe.net",
            "homepage": "http://www.mixglobe.net",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "bican/roles": "2.1.*"
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Mix359\\RolesCommands\\": "src/Mix359/RolesCommands"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mix359\\RolesCommands\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
